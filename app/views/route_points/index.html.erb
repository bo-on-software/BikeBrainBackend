<p id="notice"><%= notice %></p>

<h1>Listing Route Points</h1>

<table>
  <thead>
    <tr>
      <th>Trip</th>
      <th>Gps time</th>
      <th>Gps longitude</th>
      <th>Gps latitude</th>
      <th>Gps speed</th>
      <th>Cadence</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @route_points.each do |route_point| %>
      <tr>
        <td><%= route_point.trip %></td>
        <td><%= route_point.gps_time %></td>
        <td><%= route_point.gps_longitude %></td>
        <td><%= route_point.gps_latitude %></td>
        <td><%= route_point.gps_speed %></td>
        <td><%= route_point.cadence %></td>
        <td><%= link_to 'Show', route_point %></td>
        <td><%= link_to 'Edit', edit_route_point_path(route_point) %></td>
        <td><%= link_to 'Destroy', route_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Route point', new_route_point_path %>
